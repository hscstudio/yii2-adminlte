{
    "name": "hscstudio/yii2-adminlte",
    "description": "AdminLTE Theme for Yii Framework 2.0",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Hafid Mukhlasin",
            "email": "hafidmukhlasin@gmail.com"
        }
    ],
	"minimum-stability": "dev",
	"prefer-stable": true,
    "require": {
		"yiisoft/yii2": "*",
		"yiisoft/yii2-imagine": "*",
		"almasaeed2010/adminlte": "~2.0",
		"kartik-v/yii2-widgets": "*",
		"kartik-v/yii2-icons": "*"
    },
    "autoload": {
        "psr-4": {
            "hscstudio\\adminlte\\": ""
        }
    },
	"extra": {
        "bootstrap": "hscstudio\\adminlte\\Bootstrap"
	}
}
